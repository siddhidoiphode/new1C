#include <stdio.h>
int main(void) 
{
int size,i,flag=0;
int lis[15],search;
printf("Enter size of array: \n");
  scanf("%d",&size);
printf("\n Enter elements : \n");
for(i=0;i<size;i++)
  scanf("%d",&lis[i]);
printf("\n Enter element to be searched: ");
  scanf("%d",&search);
for(i=0;i<size;i++)
  if(search==lis[i])
  {
  printf("\n Element found at %d position",i+1);   
  }
    return 0;
}
